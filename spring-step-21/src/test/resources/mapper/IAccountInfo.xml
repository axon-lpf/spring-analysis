<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bugstack.springframework.test.dao.IAccountInfo">

    <select id="queryUserInfoById" parameterType="java.lang.String" resultType="com.bugstack.springframework.test.AccountInfo">
        SELECT name,balance
        FROM account
        where name = #{name}
    </select>

    <select id="queryUserList" parameterType="com.bugstack.springframework.test.AccountInfo" resultType="com.bugstack.springframework.test.AccountInfo">
        SELECT name,balance
        FROM account

    </select>

</mapper>